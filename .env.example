# Porkbun API Configuration
# Get your API keys from: https://porkbun.com/account/api

# Required: Your Porkbun API key
export PORKBUN_API_KEY=YOUR_PORKBUN_API_KEY

# Required: Your Porkbun secret API key
export PORKBUN_SECRET_API_KEY=YOUR_PORKBUN_SECRET_KEY

# Optional: Porkbun API base URL (default: https://api.porkbun.com/api/json/v3)
PORKBUN_BASE_URL=https://api.porkbun.com/api/json/v3

# Optional: Request timeout in seconds (default: 30)
TIMEOUT=30
